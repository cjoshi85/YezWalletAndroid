{
  "name": "YezWallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "rn-nodeify --install buffer,stream,child_process --hack && npm install --save node-libs-react-native",
    "test": "./node_modules/.bin/jest -v && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepare": "patch-package"
  },
  "dependencies": {
    "@cityofzion/neon-js": "^3.9.2",
    "apsl-react-native-button": "^3.0.0",
    "axios": "^0.18.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "bigi": "^1.4.2",
    "bignumber.js": "^7.2.1",
    "bs58": "^4.0.1",
    "bs58check": "^2.0.2",
    "buffer": "^4.9.1",
    "crypto-js": "^3.1.9-1",
    "ecurve": "^1.0.5",
    "elliptic": "^6.4.0",
    "firebase": "^3.9.0",
    "lodash": "^4.17.10",
    "long": "^3.2.0",
    "moment": "^2.22.2",
    "node-libs-react-native": "^1.0.2",
    "prop-types": "^15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "^0.48.4",
